	<parameter name="P_MaHD" class="java.lang.String"/>
	<queryString>
		<![CDATA[select 
	HoaDon.MaHoaDon,
	NgayLap,
	TienMonAn,
	TienGiamDiemThuong,
	TienGiamKhuyenMai,
	TongTien,
	TenMonAn,
	SoLuong,
	DonGia,
	SUM(SoLuong*DonGia) as ThanhTien,
	PhieuDatBan.MaPhieuDatBan,
	TenNhanVien,
	TenKhachHang 
from ChiTietGM 
join HoaDon  on HoaDon.MaPhieuDatBan= ChiTietGM.MaPhieuDatBan
join MonAn  on MonAn.MaMonAn=ChiTietGM.MaMonAn
join PhieuDatBan  on PhieuDatBan.MaPhieuDatBan=ChiTietGM.MaPhieuDatBan
join KhachHang on KhachHang.MaKhachHang=PhieuDatBan.MaKhachHang
join NhanVien  on NhanVien.MaNhanVien=HoaDon.MaNhanVien
WHERE 
	 "HoaDon"."MaHoaDon" = $P{P_MaHD}
group by 
	HoaDon.MaHoaDon,
	NgayLap,
	TienMonAn,
	TienGiamDiemThuong,
	TienGiamKhuyenMai,
	TongTien,
	TenMonAn,
	SoLuong,
	DonGia,
	PhieuDatBan.MaPhieuDatBan,
	TenNhanVien,
	TenKhachHang]]>
	</queryString>
